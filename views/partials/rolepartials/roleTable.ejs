<div class="container" style="margin-top: 100px;">
    <h1>Roles</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Role Name</th>
            </tr>
        </thead>
        <tbody>
            <% roles.forEach(function(role) { %>
                <tr>
                    <td><%= role.id %></td>
                    <td><%= role.name %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
